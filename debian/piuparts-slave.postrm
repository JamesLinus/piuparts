#!/bin/sh

set -e 

user=piupartss

case "$1" in
  purge)
    rm -rf /var/lib/piuparts/$user

    # belongs to piuparts-master atm
    #if [ -d /etc/piuparts ] ; then
    #    rm -rf /etc/piuparts
    #fi

    ;;
  upgrade|remove|failed-upgrade|abort-install|abort-upgrade)
    ;;
  *)
    echo "postrm called with unknown argument \`$1'" >&2
    exit 1
    ;;
esac

